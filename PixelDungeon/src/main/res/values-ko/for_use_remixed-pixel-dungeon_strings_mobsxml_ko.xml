  <string name="RottingFist_Desc">고대 신의 썩어가는 주먹</string>
  <string name="BurningFist_Name">불타는 주먹</string>
  <string name="BurningFist_Gender">남성</string>
  <string name="BurningFist_Name_Objective">불타는 주먹</string>
  <string name="BurningFist_Desc">고대 신의 불타는 주먹</string>
  <string name="Larva_Name">신의 애벌레</string>
  <string name="Larva_Gender">여성</string>
